{
  "InitializrApi": {
    "Logo": "Resources/logo.txt",
    "NetCoreToolService": {
      "Uri": "http://net-core-tool-service/api"
    }
  },
  "spring": {
    "application": {
      "name": "SteeltoeInitializr"
    },
    "cloud": {
      "config": {
        "uri": "http://initializr-config-server/",
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe.InitializrApi": "Information"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator":{
        "Exposure": {
          "Include": [ "*" ]
        }
      }
    }
  }
}
