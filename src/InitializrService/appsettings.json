{
  "InitializrService": {
    "Logo": "Resources/logo.txt",
    "NetCoreToolService": {
      "Uri": "http://net-core-tool-service/api"
    }
  },
  "Spring": {
    "Application": {
      "Name": "SteeltoeInitializr"
    },
    "Cloud": {
      "Config": {
        "Uri": "http://initializr-config-server/",
        "Label": "main"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe.InitializrService": "Information"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator":{
        "Exposure": {
          "Include": [ "*" ]
        }
      }
    }
  }
}
